<h1>Vehicle Index</h1>

<%= form_tag(vehicles_path, method: :get) do %>
  <%= text_field_tag :term, params[:term] %>
  <%= submit_tag 'Search', name: nil%>
<% end %>
<br>

<%= render "shared/list", vehicles: @vehicles %>
<div
  id="map"
  style="width: 50%;
  height: 300px;"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
  data-markers="<%= @markers.to_json %>">
</div>
